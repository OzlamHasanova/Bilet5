@using Core.Entities;
@model TeamMember

@{
    ViewData["Title"] = "Detail";
}


<h1>Detail</h1><br /><br />

<div class="col-4">
	<img src="~/assets/img/team//@Model.Image" height="100px" /><br /><br />
	<h3>Name: @Model.Name</h3>
	<h4>Position: @Model.Position</h4>
	<h4>Description: @Model.Description</h4><br />
	<a class="btn btn-info" asp-action="Index">Back</a>
</div>

